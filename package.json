    "dist",
    "src",
    "types",
    "packages"
  ],
  "keywords": [
    "Vue",
    "UI",
    "Components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Linkontoask/v-easy-components.git"
  },
  "style": "dist/theme-chalk/index.css",
  "main": "index.js",
  "typings": "types/index.d.ts",
